# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
marlin_main.hex \

SECONDARY_SIZE += \
marlin_main.siz \


# All Target
all: marlin_main.elf secondary-outputs

# Tool invocations
marlin_main.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU ARM Cross C++ Linker'
	arm-none-eabi-g++ -mcpu=cortex-m3 -mthumb --specs=nosys.specs -O0 -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -ffreestanding -fno-move-loop-invariants -Wall -Wextra  -g3 -T mem.ld -T libs.ld -T sections.ld -nostartfiles -Xlinker --gc-sections  -L"../system" -Wl,-Map,"marlin_main.map" --specs=nano.specs -o "marlin_main.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

marlin_main.hex: marlin_main.elf
	@echo 'Invoking: GNU ARM Cross Create Flash Image'
	arm-none-eabi-objcopy -O ihex "marlin_main.elf"  "marlin_main.hex"
	arm-none-eabi-objcopy -I ihex --output-target=binary "marlin_main.hex" "marlin_main.bin"
	@echo 'Finished building: $@'
	@echo ' '

marlin_main.siz: marlin_main.elf
	@echo 'Invoking: GNU ARM Cross Print Size'
	arm-none-eabi-size --format=berkeley "marlin_main.elf"
	@echo 'Finished building:marlin_main.bin'
	@echo  .
	@echo  Verify success!  

# Other Targets
clean:
	-$(RM) $(CC_DEPS)$(C++_DEPS)$(OBJS)$(C_UPPER_DEPS)$(CXX_DEPS)$(SECONDARY_FLASH)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_UPPER_DEPS)$(C_DEPS)$(CPP_DEPS) marlin_main.elf
	-@echo ' '

secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_SIZE)

